toml.Any(toml.Null{}) move_mutex;

void main__init() {
}

void motor__init() {
}

void sensor__init() {
}

task task  main__move_square() {
	while(true) {
		acquire(move_mutex);
		motor__forward(ac, 75);
		sleep_ms(1000);
		motor__reverse(motor_c, 75);
		sleep_ms(500);
		release(move_mutex);
	}
}

task task  main__check_sensors() {
	while(true) {
		if(sensor__read(i1) == 1) {
			acquire(move_mutex);
			motor__reverse(ac, 75);
			sleep_ms(500);
			motor__forward(a, 75);
			sleep_ms(500);
			release(move_mutex);
		}
	}
}

task main() {
	motor__init();
	sensor__init();
	
	precedes(move_square, check_sensors);
	sensor__set_touch(i1);
}
